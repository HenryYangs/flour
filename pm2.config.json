{
  "apps": [
    {
      "name": "flour",
      "cwd": "./",
      "script": "./node_modules/.bin/ts-node",
      "args": "-T -r tsconfig-paths/register index.ts",
      "exec_interpreter": "node",
      "exec_mode": "cluster",
      "error_file": "logs/error.log",
      "out_file": "logs/output.log",
      "log_file": "logs/info.log"
    }
  ]
}